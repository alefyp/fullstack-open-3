{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "prefer-destructuring": 0,
    "comma-dangle": 0,
    "consistent-return": 0
  }
}
